<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:application name="sesame-service"/>

    <dubbo:registry address="multicast://192.168.0.12:18888"/>

    <dubbo:protocol name="dubbo" port="20000"/>


    <dubbo:service interface="org.sesame.service.interfaces.SerialNumberService"
                   ref="serialNumberServiceBean"/>

<import resource="classpath:sync-serial-number-service-bean.xml" />
</beans>